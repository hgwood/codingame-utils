  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hgwood/codingame-utils/issues"
  },
  "homepage": "https://github.com/hgwood/codingame-utils#readme",
  "devDependencies": {
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "jsdoc": "^3.4.3",
    "tape": "^4.6.3",
    "webpack": "^1.13.3"
  }
}
